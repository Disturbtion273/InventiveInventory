{
    "schemaVersion": 1,
    "id": "inventive_inventory",
    "version": "${version}",
    "name": "Inventive Inventory",
    "description": "An all in one inventory management mod. [Currently in Development]",
    "authors": [
        "Strobel273",
        "LU_XD"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/inventiveinventory",
        "issues": "https://github.com/Strobel273/InventiveInventoryIssues/issues"
    },
    "license": "All-Rights-Reserved",
    "icon": "assets/inventive_inventory/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "net.strobel.inventive_inventory.InventiveInventory"
        ],
        "modmenu": [
            "net.strobel.inventive_inventory.compat.ModMenuIntegration"
        ]
    },
    "mixins": [
        "inventive_inventory.mixins.json",
        "inventive_keybindfix.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "fabric": "*",
        "minecraft": "${minecraft_version}"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.donate": "https://paypal.me/strobel273"
            }
        }
    }
}
